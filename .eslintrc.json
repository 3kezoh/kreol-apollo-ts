{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "quotes": ["error", "double"],
    "linebreak-style": ["error", "windows"],
    "no-console": "off",
    "no-shadow": "off",
    "no-underscore-dangle": "off"
  }
}
