{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "*": ["*"],
      "@api": ["./api/index.js"],
      "@api/*": ["./api/*"],
      "@components": ["./api/components/index.js"],
      "@components/*": ["./api/components/*"],
      "@utils": ["./api/utils/index.js"],
      "@utils/*": ["./api/utils/*"],
      "@Auth": ["./api/components/auth/index.js"],
      "@Auth/*": ["./api/components/auth/*"],
      "@Definition": ["./api/components/definition/index.js"],
      "@Definition/*": ["./api/components/definition/*"],
      "@Report": ["./api/components/report/index.js"],
      "@Report/*": ["./api/components/report/*"],
      "@User": ["./api/components/user/index.js"],
      "@User/*": ["./api/components/user/*"],
      "@Vote": ["./api/components/vote/index.js"],
      "@Vote/*": ["./api/components/vote/*"],
      "@@apollo": ["./apollo/index.js"],
      "@@apollo/*": ["./apollo/*"],
      "@bin": ["./bin/index.js"],
      "@bin/*": ["./bin/*"],
      "@config": ["./config/index.js"],
      "@config/*": ["./config/*"],
      "@directives": ["./directives/index.js"],
      "@directives/*": ["./directives/*"],
      "@middlewares": ["./middlewares/index.js"],
      "@middlewares/*": ["./middlewares/*"],
      "@scalars": ["./scalars/index.js"],
      "@scalars/*": ["./scalars/*"]
    }
  },
  "exclude": ["node_modules"]
}
